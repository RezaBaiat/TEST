{
  "private": true,
  "scripts": {
    "start": "react-native start",
    "ios": "SET ENVFILE=.env.staging && react-native run-ios",
    "ios:prod": "SET ENVFILE=.env.production && react-native run-ios",
    "android": "SET ENVFILE=.env.staging && react-native run-android",
    "android:prod": "SET ENVFILE=.env.production && react-native run-android",
    "android:release": "cd android && ENVFILE=.env.production ./gradlew assembleRelease",
    "installDebug": "cd android && gradlew installDebug",
    "bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "installNoDev": "yarn bundle & yarn installDebug",
    "upgradeLatest": "yarn upgrade-interactive --latest",
    "ts": "tsc --watch",
    "run_mocha": "mocha",
    "run_istanbul": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "flow": "flow",
    "doc": "rd /s /q docs && mkdir docs && docco {src,test}/{**/*,*} README.md misc/tutorials/{**/*,*} -t ./misc/template/docco.jst -c ./misc/template/docco.css && echo d | xcopy /E \"misc/public\" \"docs/public\""
  },
  "dependencies": {
    "@babel/preset-flow": "^7.0.0",
    "@types/react": "^16.7.0",
    "@types/react-native": "^0.57.8",
    "babel-polyfill": "^6.26.0",
    "chai": "^4.2.0",
    "docco": "^0.8.0",
    "es6-symbol": "^3.1.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "mocha": "^6.0.2",
    "react": "16.7.0",
    "react-native": "0.57.8",
    "react-native-config": "^0.11.7",
    "react-native-dev-kit": "https://github.com/pixelsandcode/reactnative-dev-kit.git",
    "react-native-elements": "^1.1.0",
    "react-native-fetch-mock": "^0.8.0",
    "react-native-navigation": "^2.14.0",
    "react-native-vector-icons": "^6.4.2",
    "react-redux": "^6.0.0",
    "react-timer-mixin": "^0.13.4",
    "redux": "^4.0.1",
    "winston": "^3.2.1",
    "winston-transport-rollbar-3": "^3.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "eslint": "^5.15.3",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "flow-bin": "^0.95.1",
    "prettier": "^1.16.4",
    "react-scripts": "2.1.8"
  },
  "rnpm": {
    "assets": [
      "./assets/fonts"
    ]
  }
}
